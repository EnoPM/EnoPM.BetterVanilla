<?xml version="1.0" encoding="utf-8"?>
<Il2CppEnhancer xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:noNamespaceSchemaLocation="../EnoPM.Il2CppEnhancer/Schemas/ConfigFileSchema.xml">
    <Properties>
        <Property Name="GameDirectory" Value="C:\Program Files (x86)\Steam\steamapps\common\Among Us - Vanille"/>
        <Property Name="UnityProjectDirectory" Value="C:\Users\EnoPM\TheOtherRoles"/>
        <Property Name="SolutionDirectory" Value="C:\Users\EnoPM\RiderProjects\EnoPM.BetterVanilla"/>
        <Property Name="ProjectBuildDirectory" Value="$(SolutionDirectory)\EnoPM.BetterVanilla\bin\Release\net6.0"/>
        <Property Name="NugetPackagesDirectory" Value="C:\Users\EnoPM\.nuget\packages"/>
    </Properties>
    <BepInExPlugin>
        <AssemblyInput FilePath="$(ProjectBuildDirectory)\EnoPM.BetterVanilla.Mono.dll">
            <References>
                <Reference DirectoryPath="$(GameDirectory)\dotnet">
                    <Excludes>
                        <FilePath Value="clretwrc.dll"/>
                        <FilePath Value="clrjit.dll"/>
                        <FilePath Value="coreclr.dll"/>
                        <FilePath Value="dbgshim.dll"/>
                        <FilePath Value="hostpolicy.dll"/>
                        <FilePath Value="Microsoft.DiaSymReader.Native.x86.dll"/>
                        <FilePath Value="mscordaccore.dll"/>
                        <FilePath Value="mscordaccore_x86_x86_6.0.722.32202.dll"/>
                        <FilePath Value="mscordbi.dll"/>
                        <FilePath Value="mscorrc.dll"/>
                        <FilePath Value="msquic.dll"/>
                        <FilePath Value="System.IO.Compression.Native.dll"/>
                    </Excludes>
                </Reference>
                <Reference DirectoryPath="$(NugetPackagesDirectory)\amongus.gamelibs.steam\2024.6.18\build" >
                    <Excludes>
                        <FilePath Value="Assembly-CSharp-firstpass.dll"/>
                    </Excludes>
                </Reference>
                <Reference DirectoryPath="$(GameDirectory)\BepInEx\core" >
                    <Excludes>
                        <FilePath Value="dobby.dll"/>
                    </Excludes>
                </Reference>
            </References>
        </AssemblyInput>
        <AssemblyOutput FilePath="$(ProjectBuildDirectory)\EnoPM.BetterVanilla.dll"/>
    </BepInExPlugin>
    <UnityProject>
        <AssetsDirectory DirectoryPath="$(UnityProjectDirectory)\Assets"/>
        <AssetBundlesDirectory DirectoryPath="$(UnityProjectDirectory)\Assets\AssetBundles"/>
    </UnityProject>
</Il2CppEnhancer>